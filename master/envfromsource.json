{
  "$schema": "http://json-schema.org/schema#", 
  "type": "object", 
  "description": "EnvFromSource represents the source of a set of ConfigMaps", 
  "properties": {
    "prefix": {
      "type": "string", 
      "description": "An optional identifer to prepend to each key in the ConfigMap. Must be a C_IDENTIFIER."
    }, 
    "configMapRef": {
      "description": "The ConfigMap to select from", 
      "$ref": "https://raw.githubusercontent.com/garethr/openshift-json-schema/master/master/_definitions.json#/definitions/v1.ConfigMapEnvSource"
    }, 
    "secretRef": {
      "description": "The Secret to select from", 
      "$ref": "https://raw.githubusercontent.com/garethr/openshift-json-schema/master/master/_definitions.json#/definitions/v1.SecretEnvSource"
    }
  }
}